{
  "name": "contact-directory",
  "version": "1.0.0",
  "description": "Full Stack Software Engineer Sample App",
  "main": "index.js",
  "scripts": {
    "build-client": "cd client && npm run build",
    "build-server": "cd server && npm run build",
    "build": "npm run build-client && npm run build-server",
    "start": "cd server && npm start",
    "ci": "cd client && npm ci && cd ../server && npm ci",
    "watch-client": "cd client && npm start",
    "test-watch-client": "cd client && npm run test-watch",
    "test-client": "cd client && npm test",
    "test-watch-server": "cd server && npm run test-watch",
    "test-server": "cd server && npm test",
    "watch-server-start": "cd server && npm run start-dev",
    "watch-server-build": "cd server && npm run watch",
    "watch": "npm-run-all --parallel watch-server-build watch-server-start watch-client",
    "test": "npm run test-client && npm run test-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tquinlan1992/contact-directory.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tquinlan1992/contact-directory/issues"
  },
  "homepage": "https://github.com/tquinlan1992/contact-directory#readme",
  "dependencies": {
    "@types/uuid": "^8.3.1",
    "npm-run-all": "^4.1.5",
    "uuid": "^8.3.2",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@types/validate.js": "^0.11.0"
  }
}
